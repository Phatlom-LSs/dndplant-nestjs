generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model userdata {
  id       Int    @id @default(autoincrement())
  username String @default("") @db.VarChar @unique
  hashedpassword String
  createdAt DateTime @default(now())
  updateAt DateTime @updatedAt()
}
